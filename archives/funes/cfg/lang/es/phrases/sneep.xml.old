<!-- Config stuff -->

<epp:phrase name="sneep/comment:add_textarea_cols">50</epp:phrase>
<epp:phrase name="sneep/comment:add_textarea_rows">3</epp:phrase>
<epp:phrase name="sneep/comment:edit_textarea_cols">50</epp:phrase>
<epp:phrase name="sneep/comment:edit_textarea_rows">3</epp:phrase>
<!-- This can be either 1 (comments can be viewed by non-logged in users) or 0 (coments can only be viewed by logged in users) -->
<!-- you need to run generate_abstracts as well as reloading the repository before changing the below takes effect -->
<epp:phrase name="sneep/comment:repository_open">1</epp:phrase>

<epp:phrase name="sneep/note:add_textarea_cols">50</epp:phrase>
<epp:phrase name="sneep/note:add_textarea_rows">3</epp:phrase>
<epp:phrase name="sneep/note:edit_textarea_cols">50</epp:phrase>
<epp:phrase name="sneep/note:edit_textarea_rows">3</epp:phrase>

<epp:phrase name="sneep/tag:add_textarea_cols">50</epp:phrase>
<epp:phrase name="sneep/tag:add_textarea_rows">3</epp:phrase>
<epp:phrase name="sneep/tag:edit_textarea_cols">50</epp:phrase>
<epp:phrase name="sneep/tag:edit_textarea_rows">3</epp:phrase>

